	<queryString>
		<![CDATA[SELECT DATAVENCIMENTO,
       PESSOAS.CODIGO,
       PESSOAS.RAZAOSOCIAL,
       DOCUMENTONUMERO,
       PARCELANUMERO,
       DATAPAGAMENTO,
       FORMASPAGAMENTO.DESCRICAO,
       TOTALPAGAR
FROM CONTASPAGAR
JOIN PESSOAS ON(PESSOAS.CODIGO = CONTASPAGAR.FORNECEDORCODIGO)
JOIN FORMASPAGAMENTO ON(FORMASPAGAMENTO.CODIGO = CONTASPAGAR.FORMAPAGAMENTO)
WHERE DATAVENCIMENTO BETWEEN $P{dataVencimentoInicial} AND $P{dataVencimentoFinal} AND
      PESSOAS.RAZAOSOCIAL LIKE $P{fornecedorLike} AND
      CASE
         WHEN $P{dataPagamento} = 1 THEN
            DATAPAGAMENTO BETWEEN $P{dataPagamentoInicial} AND $P{dataPagamentoFinal}
         WHEN $P{dataPagamento} = 2 THEN
            TRUE
      END
AND
      CASE
         WHEN $P{situacao} = 1 THEN
            DATAPAGAMENTO IS NULL
         WHEN $P{situacao} = 2 THEN
            DATAPAGAMENTO IS NOT NULL
         ELSE
            TRUE
      END
AND
      CASE
         WHEN $P{formaPagamento} = 1 THEN
            TRUE
         WHEN $P{formaPagamento} = 2 THEN
            FORMAPAGAMENTO = $P{formaPagamentoWhere}
      END
ORDER BY DATAVENCIMENTO, PESSOAS.RAZAOSOCIAL, DOCUMENTONUMERO, PARCELANUMERO, FORMASPAGAMENTO.CODIGO]]>
	</queryString>
