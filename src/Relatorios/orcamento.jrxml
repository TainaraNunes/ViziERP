	<queryString>
		<![CDATA[SELECT ORCAMENTOS.NUMERO,
       ORCAMENTOS.DATAABERTURA,
       ORCAMENTOS.DATAVALIDADE,
       ORCAMENTOS.VALORTOTALSERVICOS,
       ORCAMENTOS.VALORDESCONTOSERVICOS,
       ORCAMENTOS.VALORTOTALPECAS,
       ORCAMENTOS.VALORDESCONTOSPECAS,
       ORCAMENTOS.VALORTOTALORCAMENTO,
       ORCAMENTOS.OBSERVACOES,
       ORCAMENTOS.VERSAO,
       PESSOAS.RAZAOSOCIAL,
       PESSOAS.CNPJ,
       PESSOAS.ENDERECO,
       PESSOAS.BAIRRO,
       PESSOAS.NUMERO,
       PESSOAS.CIDADE,
       PESSOAS.ESTADO,
       PESSOAS.CEP,
       PESSOAS.INSCRICAOESTADUAL,
       PESSOAS.EMAIL,
       PESSOAS.TELEFONE,
       CLIENTESDADOSTECNICOS.CONTATORESPONSAVEL,
       FORMASPAGAMENTO.DESCRICAO AS FORMAPAGAMENTO
FROM ORCAMENTOS
JOIN PESSOAS ON (PESSOAS.CODIGO = ORCAMENTOS.CLIENTECODIGO)
LEFT JOIN CLIENTESDADOSTECNICOS ON(PESSOAS.CODIGO = CLIENTESDADOSTECNICOS.CLIENTECODIGO)
JOIN FORMASPAGAMENTO ON (FORMASPAGAMENTO.CODIGO = ORCAMENTOS.FORMAPAGAMENTO)
WHERE ORCAMENTOS.NUMERO = $P{numeroOrcamento}]]>
	</queryString>
