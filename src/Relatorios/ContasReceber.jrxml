	<queryString>
		<![CDATA[SELECT DATAVENCIMENTO,
       PESSOAS.CODIGO,
       PESSOAS.RAZAOSOCIAL,
       DOCUMENTONUMERO,
       PARCELANUMERO,
       DATARECEBIMENTO,
       FORMASPAGAMENTO.DESCRICAO,
       TOTALRECEBER
FROM CONTASRECEBER
JOIN PESSOAS ON(PESSOAS.CODIGO = CONTASRECEBER.CLIENTECODIGO)
JOIN FORMASPAGAMENTO ON(FORMASPAGAMENTO.CODIGO = CONTASRECEBER.FORMAPAGAMENTO)
WHERE DATAVENCIMENTO BETWEEN $P{dataVencimentoInicial} AND $P{dataVencimentoFinal} AND
      PESSOAS.RAZAOSOCIAL LIKE $P{clienteLike} AND
      CASE
         WHEN $P{dataRecebimento} = 1 THEN
            DATARECEBIMENTO BETWEEN $P{dataRecebimentoInicial} AND $P{dataRecebimentoFinal}
         WHEN $P{dataRecebimento} = 2 THEN
            TRUE
      END
AND
      CASE
         WHEN $P{situacao} = 1 THEN
            DATARECEBIMENTO IS NULL
         WHEN $P{situacao} = 2 THEN
            DATARECEBIMENTO IS NOT NULL
         ELSE
            TRUE
      END
AND
      CASE
         WHEN $P{formaPagamento} = 1 THEN
            TRUE
         WHEN $P{formaPagamento} = 2 THEN
            FORMAPAGAMENTO = $P{formaPagamentoWhere}
      END
ORDER BY DATAVENCIMENTO, PESSOAS.RAZAOSOCIAL, DOCUMENTONUMERO, PARCELANUMERO, FORMASPAGAMENTO.CODIGO]]>
	</queryString>
